{
  "name": "@kim-pro-git/rabbit-reboot",
  "version": "0.1.0",
  "scripts": {
    "lint": "vue-cli-service lint",
    "start:web": "vue-cli-service serve --mode develop --skip-plugins @vue/cli-plugin-eslint",
    "start:ios": "cap run ios --no-sync",
    "start:android": "cap run android --no-sync",
    "add:ios": "cap add ios",
    "add:android": "cap add android",
    "build": "vue-cli-service build --mode develop --skip-plugins @vue/cli-plugin-eslint",
    "build:prod": "vue-cli-service build --mode production --skip-plugins @vue/cli-plugin-eslint",
    "copy:ios": "cap copy ios",
    "copy:android": "cap copy android",
    "copy": "yarn copy:ios && yarn copy:android",
    "update:ios": "cap update ios",
    "update:android": "cap update android",
    "update": "yarn update:ios && yarn update:android",
    "icon:ios": "cordova-res ios --resources resources/i --skip-config --copy",
    "icon:android": "cordova-res android --resources resources/a --skip-config --copy",
    "open:ios": "cap open ios",
    "open:android": "cap open android"
  },
  "dependencies": {
