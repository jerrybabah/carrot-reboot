    "@capacitor/ios": "^3.2.5",
    "@capacitor/keyboard": "^1.1.2",
    "@capacitor/push-notifications": "^1.0.6",
    "@ionic/vue": "^5.8.5",
    "@ionic/vue-router": "^5.8.5",
    "@kim-pro-git/carrot-reboot-backend": "1.0.0",
    "@kim-pro-git/carrot-reboot-service": "1.0.0",
    "@vant/touch-emulator": "^1.3.2",
    "@vueuse/components": "^6.7.3",
    "@vueuse/core": "^6.7.3",
    "axios": "^0.21.4",
    "cordova-plugin-facebook-connect": "^3.2.0",
    "cordova-plugin-ionic": "^5.5.1",
    "core-js": "^3.6.5",
    "dayjs": "^1.10.7",
    "firebase": "^8.10.0",
    "pinia": "2.0.0-rc.8",
